all:

install:
	cd lib/python && python setup.py install

clean:
	rm -rf *~ lib/python/oh/common/*.pyc lib/python/oh/common/*.pyo lib/python/oh/common/*~ lib/python/build

distclean: clean
	rm -rf Makefile config.h config.status config.cache config.log \
		autom4te.cache

